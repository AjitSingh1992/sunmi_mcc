<?xml version="1.0" encoding="utf-8"?>
<merge
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:showIn="@layout/row_order_overview_tablet">

    <Space
        android:id="@+id/guideColumn1Start"
        android:layout_width="140sp"
        android:layout_height="0dp"
        android:layout_marginStart="75dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/guideColumn2Start" />

    <Space
        android:id="@+id/guideColumn2Start"
        android:layout_width="190sp"
        android:layout_height="0dp"
        app:layout_constraintStart_toEndOf="@+id/guideColumn1Start"
        app:layout_constraintEnd_toStartOf="@+id/guideColumn3Start" />

    <Space
        android:id="@+id/guideColumn3Start"
        android:layout_width="125sp"
        android:layout_height="0dp"
        app:layout_constraintStart_toEndOf="@+id/guideColumn2Start"
        app:layout_constraintEnd_toStartOf="@+id/guideColumn4Start" />

    <Space
        android:id="@+id/guideColumn4Start"
        android:layout_width="115sp"
        android:layout_height="0dp"
        app:layout_constraintStart_toEndOf="@+id/guideColumn3Start"
        app:layout_constraintEnd_toStartOf="@+id/guideColumn5Start" />

    <Space
        android:id="@+id/guideColumn5Start"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="50dp"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/guideColumn4Start"
        app:layout_constraintEnd_toEndOf="parent" />

</merge>
